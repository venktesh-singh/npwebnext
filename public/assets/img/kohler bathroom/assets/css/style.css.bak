.modelPopup {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    opacity: 0;
    visibility: hidden;
    transform: scale(1.1);
    transition: visibility 0s linear 0.25s, opacity 0.25s 0s, transform 0.25s;
    z-index: 99999;
}
.modelPopup .modal-content {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background-color: white;
    padding: 1rem 1.5rem;
    width: 24rem;
    border-radius: 0.5rem;
}
.modelPopup .close-button {
    float: right;
    width: auto;
    line-height: normal;
    text-align: center;
    cursor: pointer;
    border-radius: 50px;
    background-color: #000;
    position: absolute;
    right: -10px;
    top: -10px;
    color: #fff;
    border: 1px solid #fff;
    width: 26px;
    height: 26px;
    display: flex;
    justify-content: center;
    align-items: center;
}
.modelPopup .close-button:hover {
    background: #000;
}

.modelPopup .show-modal {
    opacity: 1;
    visibility: visible;
    transform: scale(1.0);
    transition: visibility 0s linear 0s, opacity 0.25s 0s, transform 0.25s;
}
.modelPopup.show-modal {
    opacity: 1;
    visibility: visible;
    display: block;
}

.modelPopup .top-btmtext.from {
    font-size: 15px;
}

.modelPopup .top-btmtext.from label {
    display: flex;
    margin-right: 10px;
}

.modelPopup .top-btmtext.from .category {
    display: flex;
}

.modelPopup .top-btmtext.from label input[type="radio"] {
    margin-right: 6px;
}

